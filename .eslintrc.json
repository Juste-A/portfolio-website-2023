{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/jsx-runtime",
        "plugin:react/recommended",
        "standard"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-uses-vars": "error",
        "react/jsx-uses-react": "error"
    }
}